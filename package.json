    "test:coverage": "jest --bail --coverage",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix src __tests__ App.tsx",
    "lint:styles": "stylelint --fix \"src/**/*.{tx,tsx}\"",
    "format": "prettier \"src/**/*.{js,jsx,yml,yaml,ts,tsx,md,graphql,mdx}\" --write",
